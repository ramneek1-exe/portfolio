{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Next.js (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "Next.js Dev",
      "sourceMaps": true
    },
    {
      "name": "Attach to Next.js (Chrome)",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    }
  ]
}
